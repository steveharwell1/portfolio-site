{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"settings": {
		"appearanceTools": true,
		"color": {
			"defaultPalette": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"custom": false,
			"palette": [
				{
					"slug": "blue-900",
					"color": "#11222c",
					"name": "Darkest Blue"
				},
				{
					"slug": "blue-700",
					"color": "#1c5858",
					"name": "Dark Blue"
				},
				{
					"slug": "orange-400",
					"color": "#f69000",
					"name": "Orange"
				},
				{
					"slug": "blue-400",
					"color": "#53b1b1",
					"name": "Blue"
				},
				{
					"slug": "yellow-400",
					"color": "#f6c624",
					"name": "Yellow"
				},
				{
					"slug": "white",
					"color": "#FFFFFF",
					"name": "White"
				}
			]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1200px"
		},
		"spacing": {
			"blockGap": true
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "\"Merriweather Sans\",-apple-system,BlinkMacSystemFont,Segoe UI,Roboto,Oxygen-Sans,Ubuntu,Cantarell,Helvetica Neue,sans-serif",
					"slug": "merriweather-sans",
					"name": "Merriweather Sans",
					"fontFace": [
						{
							"fontFamily": "Merriweather Sans",
							"fontWeight": "100 200 300 400 500 600 700 800 900",
							"fontStyle": "normal",
							"src": [
								"file:./public/fonts/MerriweatherSans-VariableFont_wght.ttf"
							]
						},
						{
							"fontFamily": "Merriweather Sans",
							"fontWeight": "100 200 300 400 500 600 700 800 900",
							"fontStyle": "italic",
							"src": [
								"file:./public/fonts/MerriweatherSans-Italic-VariableFont_wght.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,Segoe UI,Roboto,Oxygen-Sans,Ubuntu,Cantarell,Helvetica Neue,sans-serif",
					"slug": "system-fonts",
					"name": "System fonts"
				},
				{
					"fontFamily": "Helvetica Neue, Helvetica, Arial, sans-serif",
					"slug": "helvetica-arial",
					"name": "Helvetica or Arial"
				}
			]
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "0rem"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--merriweather-sans)"
		}
	}
}