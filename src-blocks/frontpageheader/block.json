{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "stevesblocktheme/frontpageheader",
    "title": "Front Page Header",
    "category": "theme",
    "icon": "star",
    "description": "Is the header for the front page",
    "keywords": [ "header" ],
    "version": "1.0.0",
    "textdomain": "stevesblocktheme",
    "attributes": {
        "align": {
            "type": "string",
            "default": "wide"
        }
    },
    "supports": {
        "align": ["full", "wide"]
    },
    "editorScript": "file:./index.js",
    "viewScript": "file:./view.js",
    "style": "file:./style-index.css"
}